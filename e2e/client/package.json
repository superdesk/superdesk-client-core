{
  "private": true,
  "dependencies": {
    "jquery": "3.3.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "superdesk-core": "file:../../"
  },
  "devDependencies": {
    "@superdesk/build-tools": "file:../../build-tools",
    "grunt": "1.3.0",
    "http-server": "0.12.1",
    "protractor": "7.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "npx @superdesk/build-tools build-root-repo ./",
    "start-client-server": "http-server dist -p 9000 -s &",
    "stop-client-server": "fuser -k 9000/tcp",
    "protractor": "node run-end-to-end-tests.js",
    "specs--compile": "find ../../spec/ -name '*.js' -type f -delete && tsc -p ../../spec",
    "specs--watch": "find ../../spec/ -name '*.js' -type f -delete && tsc -p ../../spec -w"
  }
}
